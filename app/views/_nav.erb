<% if authenticated? %>
  <ul>
    <li><a href="/">Home</a></li>
    <li><a href="/users/<%= session[:username] %>">Profile</a></li>
    <li><a href="/questions/new">Ask Question</a></li>
    <li><a href="/logout">Logout</a></li>
  </ul>
<% else %>
  <ul>
    <li><a href="/">Home</a></li>
    <li><a href="/login">Login/Register</a></li>
  </ul>
<% end %>
